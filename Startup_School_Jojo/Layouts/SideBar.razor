@code {
    private string? view;
    private string? page;
    private async void ChangePanel(string text)
    {
        if (!string.IsNullOrEmpty(page))
        {
            view = "";
            await Task.Delay(150);
            if (page == text || text == "home")
            {
                page = "";
            }
            else
            {
                page = text;
                StateHasChanged();
                await Task.Delay(150);
                view = "view";
            }
        }
        else
        {

            page = text;
            StateHasChanged();
            await Task.Delay(20);
            view = "view";

        }
        StateHasChanged();

    }
}